{
	"name": "turbo-python-print",
	"displayName": "Turbo Python Print",
	"icon": "images/turbo-python-print-v2.png",
	"publisher": "FedrickNishant",
	"repository": "https://github.com/fedricknishant/turbo-print",
	"description": "",
	"version": "0.3.0",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:turbo-python-print.pythonPrint",
    	"onCommand:turbo-python-print.pythonRemoveAllPrint"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "turbo-python-print.pythonPrint",
				"title": "Print The line numbers and the code"
			},
			{
				"command": "turbo-python-print.pythonRemoveAllPrint",
				"title": "Turbo Print - Python - Remove All Print"
			}
		],
		"keybindings": [
			{
				"command": "turbo-python-print.pythonPrint",
				"key": "ctrl+alt+p",
				"mac": "ctrl+alt+p"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.3"
	}
}
